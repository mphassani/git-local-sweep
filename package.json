{
  "name": "@cryterion/git-local-sweep",
  "version": "1.1.2",
  "description": "Clean up local Git branches that no longer exist on remote.",
  "license": "MIT",
  "author": "Parsa Hassani",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mphassani/git-local-sweep.git"
  },
  "bugs": {
    "url": "https://github.com/mphassani/git-local-sweep/issues"
  },
  "homepage": "https://github.com/mphassani/git-local-sweep#readme",
  "bin": {
    "git-local-sweep": "git-local-sweep",
    "gls": "git-local-sweep"
  },
  "files": [
    "git-local-sweep",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "check:version-sync": "python3 scripts/check_version_sync.py",
    "version:bump": "python3 scripts/bump_version.py",
